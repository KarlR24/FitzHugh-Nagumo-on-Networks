    "import matplotlib.pyplot as plt\n",
    "\n",
    "def plot_activity(activity):\n",
    "    \"\"\"\n",
    "    Plot v vs time & w vs time for each oscillator time series.\n",
    "    activity: 2D-np.ndarray\n",
    "        Activity time series, node vs. time; ie output of FitzHugh_Nagumo_coupled.run()\n",
    "    return:\n",
    "        matplotlib axis for further customization\n",
    "    \"\"\"\n",
    "    _, ax = plt.subplots(figsize=(12, 4))\n",
    "    ax.plot(activity[0])\n",
    "    ax.set_xlabel('Time', fontsize=25)\n",
    "    ax.set_ylabel('v', fontsize=25)\n",
    "    \n",
    "    _, ax = plt.subplots(figsize=(12, 4))\n",
    "    ax.plot(activity[1])\n",
    "    ax.set_xlabel('Time', fontsize=25)\n",
    "    ax.set_ylabel('w', fontsize=25)\n",
    "    \n",
    "    return ax"
   ]
